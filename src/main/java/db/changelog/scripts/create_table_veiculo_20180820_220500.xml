<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create-veiculo-201808222205-0" author="graziela">
		<comment>Criado a tabela de ve√≠culos</comment>
		<createTable tableName="veiculo">
			<column name="id" type="bigint" valueComputed ="seq_veiculo.NEXTVAL">
                <constraints primaryKey="true" nullable="false"/>
            </column>
			<column name="modelo" type="java.sql.Types.VARCHAR(255)">
				<constraints nullable="false" />
			</column>
			<column name="fator_multiplicador" type="java.sql.Types.DECIMAL">
				<constraints nullable="false" />
			</column>
		</createTable>
	</changeSet>
	
</databaseChangeLog>